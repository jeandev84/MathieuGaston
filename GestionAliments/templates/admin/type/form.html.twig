{% extends 'base.html.twig' %}

{% block title %}Type{% endblock %}

{% block monTitre %}
    {% if isModification %}
        Modification {{ type.libelle }}
    {% else %}
        Ajout d'un type ou categorie
    {% endif %}
{% endblock %}
{% block body %}
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <input type="submit" class="btn btn-primary" value="Valider">
    {{ form_end(form) }}
{% endblock %}
